/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    And(a= a, b= sel, out= aAndsel);
    Xor(a= a, b= aAndsel, out= aXoraAndsel);
    And(a= b, b= sel, out= bAndsel);
    Xor(a= aXoraAndsel, b= bAndsel, out= out);
}
